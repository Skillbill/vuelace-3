{
  "name": "@skillbill/vuelace-3",
  "version": "1.0.2",
  "private": false,
  "author": "skillbill",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Skillbill/vuelace-3.git"
  },
  "bugs": {
    "url": "https://github.com/Skillbill/vuelace-3/issues"
  },
  "homepage": "https://github.com/Skillbill/vuelace-3#readme",
  "files": [
    "dist/**"
  ],
  "keywords": [
    "components",
    "shoelace",
    "ui library",
    "vue library"
  ],
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/vuelace.umd.cjs",
  "module": "./dist/vuelace.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/vuelace.js"
      },
      "require": "./dist/vuelace.umd.cjs"
    },
    "./styles/vuelace.css": {
      "import": "./dist/styles/index.css",
      "require": "./dist/styles/index.css"
    },
    "./styles/theme_primevue.css": {
      "import": "./dist/styles/theme.css",
      "require": "./dist/styles/theme.css"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --project .",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@storybook/addon-essentials": "^7.6.15",
    "@storybook/addon-interactions": "^7.6.15",
    "@storybook/addon-links": "^7.6.15",
    "@storybook/blocks": "^7.6.15",
    "@storybook/test": "^7.6.15",
    "@storybook/vue3": "^7.6.15",
    "@storybook/vue3-vite": "^7.6.15",
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^18.19.3",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.49.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^23.0.1",
    "npm-run-all2": "^6.1.1",
    "postcss": "^8.4.33",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "storybook": "^7.6.15",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.3.0",
    "vite": "^5.0.10",
    "vite-plugin-dts": "^3.7.1",
    "vue-tsc": "^1.8.25"
  },
  "peerDependencies": {
    "@mdi/svg": "^7.4.47",
    "@shoelace-style/shoelace": "^2.13.0",
    "pinia": "^2.1.7",
    "primevue": "^3.51.0",
    "vue": "^3.4.24"
  }
}
